                        <!--Ojo... En este h4, se puede usar una variable como parametro para llamar la tabla a visualizar??-->
                        <h4 class="modal-title" id="exampleModalLongTitle">Titulo de la Tabla</h4>

                        <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="margin-top: -20px;">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <!--/////////////////-->
                    <!--Cuerpo del popup donde va la tabla-->
                    <div class="modal-body">

                        @*<table id="Tabla" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">*@
                        <table id="example" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">
                            <thead>
                                <tr>
                                    <th>
                                        <label class="control-label">Cod. Oficial</label>
                                    </th>
                                    <th>
                                        <label class="control-label">Nombre Cobrador</label>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @if ((IEnumerable<PrestamosCompactoWEB_Cs.Models.Select_ListadoCobradores>)ViewBag.ModelCobrador != null && (IEnumerable<PrestamosCompactoWEB_Cs.Models.Select_ListadoCobradores>)ViewBag.ModelCobrador != null)
                                {
                                    foreach (var item in (IEnumerable<PrestamosCompactoWEB_Cs.Models.Select_ListadoCobradores>)ViewBag.ModelCobrador)
                                    {

                                        <tr>
                                            <td style="cursor:pointer">
                                                @Html.DisplayFor(modelItem => item.CodCobrador)
                                            </td>
                                            <td style="cursor:pointer">
                                                @Html.DisplayFor(modelItem => item.NombreCobrador)
                                            </td>
                                            @*<td>
                                                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                                                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                                                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                                            </td>*@
