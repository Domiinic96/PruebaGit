                    @Html.EditorFor(model => model.FechaDesde, new { @htmlAttributes = new { @class = "form-control inline sizedoc", @style = "width: 80%;", @Placeholder = "DD/MM/YYYY",  @Id="FechaDesde", @Onkeyup = "this.value=formateafecha(this.value,'" + ViewBag.ModelFecha + "')" } })
