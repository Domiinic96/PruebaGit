                            $('#NoDocumento').val(res[0].NoDocumento);
                            $('#TipoDocumento').val(res[0].TipoDocumento);
                            $('#Fecha').val(ToJavaScriptDate(res[0].Fecha));
                            $('#codigoCobrador').val(res[0].codigoCobrador);
                            $('#Comentario').val(res[0].Comentario);
                            $('#Pendientes').val(res[0].Pendientes);
                            $('#Pagadas').val(res[0].Pagadas);
                            $('#BalanceAlCobro').val(res[0].BalanceAlCobro);
                            $('#Capital').val(formatCurrency(res[0].Capital));
                            $('#Interes').val(formatCurrency(res[0].Interes));
                            $('#Interes').val(formatCurrency(res[0].Interes));
                            $('#Otros').val(formatCurrency(res[0].Otros));
                            $('#Mora').val(formatCurrency(res[0].Mora));
                            $('#Total').val(formatCurrency(res[0].Mora));



                            $('#Pendientes').attr('ReadOnly', true);
                            $('#Pagadas').attr('ReadOnly', true);
                            $('#Contrato').attr('Disabled', 'Disabled');
                            $('#Cedula').attr('Disabled', 'Disabled');
