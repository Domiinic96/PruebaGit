                            $('#boton3').attr('Disabled', 'Disabled');
                            $('#botonnuevo').attr('Disabled', 'Disabled');
                            $('#NotaPtm').attr('ReadOnly', true);


                            $('#Registrar').attr('Disabled', 'Disabled');
                            $('#Actualizar').attr('Disabled', false);




                        });
                    },
                    error: function (ex) {
                        alert('Failed.' + ex);
                    }
                });

                $("#ModalPrestamo").modal('hide');

